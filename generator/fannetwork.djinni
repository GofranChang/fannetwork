net_state = enum {
    SUCCESS;
    UNINITIALIZED;
    MULTI_OPERATOR;
    IO_OPERATOR_ERR;
    ILLEGAL_PARAMS;
    INTERNAL_ERR;
    LICENSE_ERR;
    MULTI_INIT;
}

event_handler = interface +c {
    on_event(fd:i32, evt:i16);
    on_read(fd:i32, msg:binary);
}

tcp_server = interface +c {
    static create(handler:event_handler):tcp_server;
    init(port:i16, thread_num:i32):net_state;
    start();
}
